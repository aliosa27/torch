
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'VHT80'
	option disabled '0'
	option channel '36'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option encryption 'psk2'
	option ssid 'TorchWifi-monitored'
	option key 'letitshine'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option htmode 'HT20'
	option disabled '0'
	option channel '11'

config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option network 'lan'
	option encryption 'psk2'
	option ssid 'TorchWifi-monitored'
	option key 'letitshine'

